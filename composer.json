{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "bugsnag/bugsnag-laravel": "^2.0",
        "emmanuel/livewire-datatable-bs4": "dev-main",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "funkjedi/composer-include-files": "^1.1",
        "guywarner/laravel-google-chat-alerts": "^2.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "halivert/laravel-js-routes": "dev-main",
        "jeroennoten/laravel-adminlte": "^3.5",
        "laravel/framework": "^8.12",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.2",
        "livewire/livewire": "^2.4",
        "mpdf/mpdf": "dev-development",
        "netojose/laravel-bootstrap-4-forms": "dev-master",
        "santigarcor/laratrust": "^6.3",
        "yajra/laravel-datatables-html": "^4.41",
        "yajra/laravel-datatables-oracle": "^9.21"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.10",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan route:tojs -f"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models -W -n"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "funkjedi/composer-include-files": true
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dansleboby/laravel-bootstrap-4-forms"
        },
        {
            "type": "vcs",
            "url": "https://github.com/dansleboby/mpdf"
        },
        {
            "type": "vcs",
            "url": "https://github.com/dansleboby/laravel-js-routes"
        }
    ],
    "extra": {
        "include_files": [
            "app/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
